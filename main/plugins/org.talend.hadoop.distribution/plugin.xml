<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.talend.core.runtime.service">
      <Service
            class="org.talend.hadoop.distribution.service.HadoopDistributionService"
            serviceId="IHadoopDistributionService">
      </Service>
   </extension>
   <extension
         point="org.talend.core.migrationTask">  
         <projecttask
              beforeLogon="true"
              breaks="7.3.1"
              class="org.talend.hadoop.distribution.migration.UpdateRemovedHadoopVersion"
              description="update version to avoid compile errors from removed hadoop distributions"
              id="org.org.talend.hadoop.distribution.migration.UpdateRemovedHadoopVersion"
              name="UpdateRemovedHadoopVersion"
              version="7.3.1">                     
       </projecttask>
   </extension>
</plugin>
