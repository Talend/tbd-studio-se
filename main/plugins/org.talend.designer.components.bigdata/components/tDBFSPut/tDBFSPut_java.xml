<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true" PARTITIONING="NONE" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>Big Data/DBFS</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" NUM_ROW="1"
			REPOSITORY_VALUE="DBFS" />
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			FILTER="tDBFSConnection" NUM_ROW="5"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true" />
			
		<PARAMETER NAME="DATABRICKS_ENDPOINT" REPOSITORY_VALUE="DATABRICKS_ENDPOINT"
			FIELD="TEXT" NUM_ROW="10"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>"https://westeurope.azuredatabricks.net"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DATABRICKS_TOKEN" REPOSITORY_VALUE="DATABRICKS_TOKEN"
			FIELD="PASSWORD" REQUIRED="true" NUM_ROW="15"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="LOCALDIR" FIELD="DIRECTORY" REQUIRED="false"
			NUM_ROW="20">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DATABRICKS_DBFS_FOLDER" REPOSITORY_VALUE="DATABRICKS_DBFS_FOLDER"
			FIELD="TEXT" REQUIRED="true" NUM_ROW="25">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="OVERWRITE" FIELD="CLOSED_LIST" NUM_ROW="30">
			<ITEMS DEFAULT="false">
				<ITEM NAME="NEVER" VALUE="false" />
				<ITEM NAME="ALWAYS" VALUE="true" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="FILES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="40" NB_LINES="5">
			<ITEMS>
				<ITEM NAME="FILEMASK" />
				<ITEM NAME="NEWNAME" VALUE="&quot;&quot;" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="120">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>
	<ADVANCED_PARAMETERS>

	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS REQUIRED_IF="(USE_EXISTING_CONNECTION=='false')">
            <IMPORT NAME="log4j-1.2.17" MODULE="log4j-1.2.17.jar" MVN="mvn:log4j/log4j/1.2.17"/>
            <IMPORT NAME="talend-bigdata-launcher-platform-1.2.8-20180926.jar"
                    MODULE="talend-bigdata-launcher-platform-1.2.8-20180926.jar"
                    MVN="mvn:org.talend.libraries/talend-bigdata-launcher-platform-1.2.8-20180926/6.0.0"/>
            <IMPORT NAME="json-simple-1.1.1.jar" MODULE="json-simple-1.1.1.jar"
                    MVN="mvn:com.googlecode.json-simple/json-simple/1.1.1"/>
            <IMPORT NAME="javax.ws.rs-api-2.0-m10.jar" MODULE="javax.ws.rs-api-2.0-m10.jar"
                    MVN="mvn:org.talend.libraries/javax.ws.rs-api-2.0-m10/6.0.0"/>
            <IMPORT NAME="commons-codec-1.4.jar" MODULE="commons-codec-1.4.jar"
                    MVN="mvn:commons-codec/commons-codec/1.4"/>
            <IMPORT NAME="cxf-rt-frontend-jaxrs-3.3.4.jar" MODULE="cxf-rt-frontend-jaxrs-3.3.4.jar"
                    MVN="mvn:org.apache.cxf/cxf-rt-frontend-jaxrs-3.3.4"/>
            <IMPORT NAME="cxf-api-3.3.4.jar" MODULE="cxf-api-3.3.4.jar"
                    MVN="mvn:org.apache.cxf/cxf-api-3.3.4"/>
            <IMPORT NAME="cxf-rt-bindings-xml-3.3.4.jar" MODULE="cxf-rt-bindings-xml-3.3.4.jar"
                    MVN="mvn:org.apache.cxf/cxf-rt-bindings-xml-3.3.4"/>
            <IMPORT NAME="cxf-rt-core-3.3.4.jar" MODULE="cxf-rt-core-3.3.4.jar"
                    MVN="mvn:org.apache.cxf/cxf-rt-core-3.3.4"/>
            <IMPORT NAME="cxf-rt-transports-http-3.3.4.jar" MODULE="cxf-rt-transports-http-3.3.4.jar"
                    MVN="mvn:org.apache.cxf/cxf-rt-transports-http-3.3.4"/>
            <IMPORT NAME="wsdl4j-1.6.3.jar" MODULE="wsdl4j-1.6.3.jar" MVN="mvn:wsdl4j/wsdl4j/1.6.3"/>
			<IMPORT NAME="gson-2.2.4.jar" MODULE="gson-2.2.4.jar" MVN="mvn:com.google.code.gson/gson/2.2.4"/>
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_FILE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="TRANSFER_MESSAGES" TYPE="id_String" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
